<p id="notice"><%= notice %></p>

<h1>Lotes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lotes.each do |lote| %>
      <tr>
        <td><%= link_to lote.data.strftime("%m/%d/%Y"), lote %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
